{
  "name": "stitch-sagespace-v1",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "clean": "rm -rf dist node_modules/.vite",
    "dev": "vite",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "security:audit": "npm audit",
    "security:audit:all": "npm audit && cd api && npm audit",
    "security:audit:api": "cd api && npm audit",
    "security:fix": "npm audit fix",
    "security:fix:api": "cd api && npm audit fix",
    "setup:sentry": "node scripts/setup-sentry.js",
    "test": "vitest run",
    "test:api": "vitest run tests/api",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:integration": "vitest run tests/integration",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run tests/unit",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@edge-runtime/vm": "latest",
    "@emotion/is-prop-valid": "latest",
    "@google/genai": "latest",
    "@modelcontextprotocol/sdk": "latest",
    "@opentelemetry/api": "latest",
    "@sentry/react": "latest",
    "@supabase/supabase-js": "latest",
    "@tanstack/react-query": "latest",
    "@testing-library/dom": "latest",
    "@vitest/browser-playwright": "latest",
    "@vitest/browser-preview": "latest",
    "@vitest/browser-webdriverio": "latest",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "latest",
    "happy-dom": "latest",
    "immer": "latest",
    "jiti": "latest",
    "k6": "latest",
    "less": "latest",
    "lightningcss": "latest",
    "lucide-react": "^0.454.0",
    "react": "^18.3.1",
    "react-dom": "latest",
    "react-router-dom": "latest",
    "sass": "latest",
    "sass-embedded": "latest",
    "stripe": "latest",
    "stylus": "latest",
    "sugarss": "latest",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "terser": "latest",
    "tsx": "latest",
    "use-sync-external-store": "latest",
    "yaml": "latest",
    "zustand": "latest"
  },
  "devDependencies": {
    "@axe-core/react": "^4.11.0",
    "@playwright/test": "^1.57.0",
    "@sentry/nextjs": "^10.27.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^22",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.14",
    "@vitest/ui": "^4.0.14",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jest-axe": "^10.0.0",
    "jsdom": "^27.2.0",
    "next": "16.0.7",
    "playwright": "^1.57.0",
    "postcss": "^8.5",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.17",
    "typescript": "^5",
    "vite": "^5.0.8",
    "vitest": "^4.0.14"
  }
}